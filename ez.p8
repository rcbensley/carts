function roll()
 return flr(rnd(6)+1)
end

function nroll()
 result={}
	c = 1
 d = roll()
	result[c]=d
	while(d>=6) do
		d = roll()
		c+=1
		result[c]=d
	end

	return result
end


t=nroll()
for i in all(t) do
	print(i)
end
