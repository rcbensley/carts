version 32
__lua__
function _init() 
	s={}
	for x=1,256 do
	 a = rnd(512)
		add(s,{a,rnd(128),7})
	end
end


function _draw()
	
	cls()
	for i=1,#s do
		pset(s[i][1],s[i][2],s[i][3])
		s[i][1] = s[i][1] - 1
		
	end

end


