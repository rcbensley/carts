__lua__
-- random team generator
-- rcbensley

function _init()
	r = {}
	r.said = {}
	names = {
		"sledge",
		"thatcher",
		"ash",
		"thermite",
		"twitch",
		"montagne",
		"glaz",
		"fuze",
		"blitz",
		"iq",
		"smoke",
		"mute",
		"pulse",
		"castle",
		"doc",
		"rook",
		"kapkan",
		"tachanka",
		"jager",
		"bandit"
	}
	r.ops = {}
	r.blue = {}
	r.orange = {}
	r.startup = 1
	
	say = {
		"ez", "gg", "buy an ssd!",
		"reported", "lol tryhard",
		"ban cav", "lol",
		"u suck noob",	"get armour!",
		"watch for peeks", "ez pz",
		"git gud",	"go play ranked",
		"vote kick plz", "plz go mute",
	}
	r.last = time()
	resetops()
	resetchat()

end

function shuffle(t)
  for i = #t, 1, -1 do
    local j = flr(rnd(i)) + 1
    t[i], t[j] = t[j], t[i]
  end
end

function settrash()
	for i=1,3 do
		r.said[i] = r.said[i+1]
	end	
		r.said[4] = trashrow()
end

function trashrow()
 i = flr(rnd(10)) + 1
 j = flr(rnd(#say)) + 1
	s = "[all] " .. names[i] .. ": " .. say[j]
	return s
end

function resetchat()
	for i=1,4 do
		r.said[i] = trashrow()
	end
end

function resetops()
	shuffle(names)
	for i=1,10 do
		r.ops[i] = names[i]
	end
	r.new = true
end

function shuffleops()
	shuffle(r.ops)
	r.new = false
end


function updateteams()
	if btnp(4) then
		resetops()
		r.new=true
	end
	if btnp(5) then
		shuffleops()
	end
end

function printteams()
	sr = 16
	bc = 12
	oc = 9

	if r.new == true then
			print("choose an operator",
				16, sr-4, 7)
	 for i=1,5 do
	  b = i .. "." .. r.ops[i]
	 	print(b,
	 		16,sr+(i*8), 7)
	 end
	
	 for i=1,5 do
	  o = i+5 .. "." .. r.ops[i+5]
	 	print(o,
	 		64,sr+(i*8), 7)
	 end
	else
		-- print teams
		print("blue team",
			16,sr-4, 12)
		print("orange team",
				64, sr-4, 9)
	 for i=1,5 do
	  b = i .. "." .. r.ops[i]
	 	print(b,
	 		16,sr+(i*8), 12)
	 end
	
	 for i=1,5 do
	  o = i .. "." .. r.ops[i+5]
	 	print(o,
	 		64,sr+(i*8), 9)
	 end
 end
 --print trash
	tr = 68
	for i=1,4 do
		print(r.said[i],
			1, tr+(i*9), 7)
	end
	-- buttons line
	print("🅾️ new, ❎ shuffle",
		22, 118, 7)
end

function _update()
	updateteams()
end

function _draw()
	cls()
	rectfill(0, 0, 128, 128, 1)
	if (time() - r.last) > 4 then
		settrash()
		r.last = time()
	end
	printteams()
end

