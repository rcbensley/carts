pico-8 cartridge // http://www.pico-8.com
version 32
__lua__
function fc(t)
	for x=1,5000 do
		add(t,{rnd(128),rnd(128)})
	end
end

function fd(t,c)
	for i in all(t) do
		pset(i[1],i[2],c)
	end
end

function _init()
	a={}
	b={}
	fc(a)
	fc(b)
end


function _draw()
	cls()
	circfill(64,64,24,9)
	fd(a,0)
	circfill(64,64,20,10)
	fd(b,0)
	circfill(64,64,16,1)
end
